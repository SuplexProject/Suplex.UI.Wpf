<Window x:Class="Suplex.UI.Wpf.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:local="clr-namespace:Suplex.UI.Wpf"
        Height="1000" Width="1300"
        >
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*" />
        </Grid.ColumnDefinitions>

        <TabControl>
            <TabItem Header="Secure Objects">
                <local:SecureObjectDlg x:Name="dlgSecureObjects" />
            </TabItem>
            <TabItem Header="Security Principals" IsSelected="True">
                <local:SecurityPrincipalDlg x:Name="dlgSecurityPrincipals" />
            </TabItem>
        </TabControl>
    </Grid>
</Window>
